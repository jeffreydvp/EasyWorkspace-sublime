[
    // open easy workspace
    {
        "keys": ["ctrl+alt+o"],
        "command": "open_easy_workspace"
    },

    // save easy workspace
    {
        "keys": ["ctrl+alt+s"],
        "command": "save_easy_workspace"
    },

    // save as easy workspace
    {
        "keys": ["ctrl+shift+alt+s"],
        "command": "save_as_easy_workspace"
    }
]